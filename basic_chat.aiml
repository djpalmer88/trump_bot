         <srai><star/> HELLO </srai>
      </template>
    </category>
    <category>
      <pattern>* HEY *</pattern>
      <template>
         <srai><star/> HELLO </srai>
      </template>
    </category>
    <category>
      <pattern>* HI *</pattern>
      <template>
